{
  "name": "humane-node-commons",
  "version": "1.2.0",
  "description": "Commons module for humane discovery",
  "repository": "git@github.com:360fy/humane-node-commons.git",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.4.1",
    "hiredis": "0.5.0",
    "lodash": "4.13.1",
    "redis": "2.6.2",
    "redis-sentinel": "0.3.3",
    "request": "2.73.0"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-node5": "11.1.0",
    "eslint": "3.1.0",
    "eslint-config-360fy": "1.1.10",
    "eslint-plugin-react": "5.2.2"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  }
}
