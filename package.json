{
  "name": "humane-node-commons",
  "version": "1.0.0",
  "description": "Commons module for humane node",
  "repository": "git@github.com:360fy/humane-node-commons.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.4",
    "hiredis": "^0.4.1",
    "lodash": "^4.6.1",
    "redis": "^2.5.2",
    "redis-sentinel": "^0.3.3",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-node5": "^11.0.0",
    "eslint": "^2.4.0",
    "eslint-config-360fy": "^1.0.1",
    "eslint-plugin-react": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  }
}
