    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-node5": "^11.1.0",
    "eslint": "^2.9.0",
    "eslint-config-360fy": "^1.1.5",
    "eslint-plugin-react": "^5.1.1"
  },
  "eslintConfig": {
